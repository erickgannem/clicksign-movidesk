{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",  
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "isolatedModules": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build/",
    "noImplicitAny": true,
  },
}
